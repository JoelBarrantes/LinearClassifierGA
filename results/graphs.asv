function output = 
output = zeros(11,5);
best_acc = zeros(3,1);
best_loss = zeros(3,1);
for i = 1:3
    matFileName = sprintf('cifar_25_5_a_r%d.txt', i);
    m = load(matFileName);
    best_acc(i) = m(11,4);
    best_loss(i) = m(11,5);
    output = output + m;
    
end 

prom = output/3;
prom(11,1) = prom(11,1)+1;

plot(prom(:,1),prom(:,3));
hold on;


